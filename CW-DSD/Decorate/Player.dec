actor CWDM_isShotgunner : ClassToken
{
}


actor CWDM_shotgunnerClass : CWDM_player
{
	Player.DisplayName "Shotgunner"
	Player.StartItem "CWDM_Shotgun"
	Player.StartItem "CWDM_isShotgunner"
	Player.ColorRange 179, 191
	Player.Portrait "SGPORTR8"

	States
	{
	Spawn:
		SPOS D -1 
		Loop
	See:
		SPOS ABCD 4
		loop
	Pain:
		TNT1 A 0 A_Jump(128, "Pain2", "Pain3", "Pain4")
		SPOS G 15 
		Goto see
	Pain2:	
		SPOS X 15 	
		Goto See
	Pain3:	
		SPOS Y 15 
		Goto See
	Pain4:	
		SPOS Z 15 
		Goto See
	Melee:
		SPOS E 4
		SPOS F 4 BRIGHT
		goto spawn
	}
}

