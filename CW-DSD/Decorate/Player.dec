actor CWDM_bossClass : CWDM_player
{
	+NOSKIN
	//Health 500
}

// Stanley Steel Press
actor CWDM_stanClass : CWDM_bossClass
{
	Player.DisplayName "Stanley Steel Press"
	Height 64
	Player.ViewHeight 48.0
	States
	{
	Spawn:
		STAF A -1 
		Loop
	See:
		STAF BCDEFD 3
		loop
	Missile:
		STAN H 6 BRIGHT
		STAN IJ 6
		goto see
	Melee:
		STAN H 6 BRIGHT
		STAN IJ 6
		goto see
	Pain:
		STAN R 4
		goto see
	Death:
		SPOS L 0 ACS_NamedExecuteAlways("stanDied",0)
		STAM ABCDEFGHIJKLM 4
		STAM M -1
		Loop
	}
}

actor CWDM_smasherClass : CWDM_bossClass
{
	Player.DisplayName "Sunken Smasher"
	Height 72.0
	Player.ViewHeight 64.0
	Speed 0.75
	player.jumpz 0
	States
	{
	Spawn:
		SMAS A -1 
		Loop
	See:
		SMAS BCDEFG 8
		loop
	Missile:
		SMAS A 1
		goto see
	Melee:
		SMAS NNO 4
		goto see
	Pain:
		SMAS A -1 
		goto see
	Death:
		SPOS L 0 ACS_NamedExecuteAlways("smasherDied",0)
		STAM ABCDEFGHIJKLM 4
		STAM M -1
		Loop
	}
}

actor CWDM_EARLClass : CWDM_bossClass
{
	Player.DisplayName "EARL"
	Height 72.0
	Speed 0.6
	Player.ViewHeight 64.0
	States
	{
	Spawn:
		EARL A -1 
		Loop
	See:
		EARL BC 8
		loop
	Missile:
		EARL E 4
		EARL FG 4 BRIGHT
		goto see
	Melee:
		EARL E 4
		EARL FG 4 BRIGHT
		goto see
	Death:
		EARL A 0 ACS_NamedExecuteAlways("earlDied",0)
		goto FullDeath
	FullDeath:
		EARL HI 4
		Loop
	}
}
