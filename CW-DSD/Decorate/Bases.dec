actor CWDM_player : PlayerPawn 
{
	Speed 0.9
	Health 100
	Radius 20
	Height 60
	Mass 90
	Gravity 1.0
	PainChance 255
	Player.ColorRange 114, 124
	player.jumpz 10
	DropItem "CWDM_deathPack"
	+DONTGIB
	States
	{
	Death:
		TNT1 A 0 A_SpawnItemEx("CWDM_deathPack", 0, 0, 16, 0, 0, 5, 0, SXF_NOCHECKPOSITION)
		TNT1 AAAAA 0 A_SpawnItemEx(
			"CWDM_Blood",
			0, 0, 16,
			Random(-2,2),
			Random(-2,2),
			Random(2,4),
			0,
			SXF_NOCHECKPOSITION
		)
		TNT1 AAAAA 0 A_SpawnItemEx(
			"CWDM_gib",
			0, 0, 16,
			Random(-2,2),
			Random(-2,2),
			Random(6,10),
			0,
			SXF_NOCHECKPOSITION
		)
		TNT1 A -1
		stop
	FullDeath:
		goto death
	Death.extreme:
		goto death
	}
}

ACTOR CWDM_ammo : Ammo 
{

}

ACTOR CWDM_weapon : Weapon 
{
	Weapon.BobRangeX 0.25
	Weapon.BobRangeY 0.5
	Weapon.BobSpeed 2
	Weapon.BobStyle InverseSmooth
	Weapon.SelectionOrder 350
	Inventory.PickupSound "misc/usgpickup"
	+AMMO_OPTIONAL
	+WEAPON.NOAUTOAIM
	Weapon.SwayStyle "HorizontalOnly"
	Weapon.ViewSwaySpeed 2.5
	Weapon.Kickback 150
	+INVENTORY.UNDROPPABLE
	+INVENTORY.UNTOSSABLE
}

